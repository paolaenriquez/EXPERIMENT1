#include <iostream>
#include <conio.h>

using namespace std;

int main()

{
	cout << "Ticket Price / Ticket Sold" << endl ;
	cout << "$250			5750" << endl ;
	cout << "$100			28000" << endl ;
	cout << "$50			35750" << endl ;
	cout << "$25			25750" << endl ;

	cout << "TotalTickets = " << (250 * 5750) + (100 * 28000) + (50 * 35750) + (25 * 18750) << endl;

	_getch();

	return 0;
}
